    "description": "Frontend boilerplate",
    "main": "index.js",
    "scripts": {
        "start": "pm2 start config/pm2/dev.yml & pm2 logs",
        "build": "npm-run-all build:vendor build:theme build:client ",
        "build:vendor": "cross-env NODE_ENV=production gulp vendor",
        "build:theme": "cross-env NODE_ENV=production gulp theme",
        "build:servers": "cross-env NODE_ENV=production SERVER=production gulp servers",
        "build:client": "cross-env NODE_ENV=production gulp client",
        "stop": "pm2 delete all",
        "schema2json": "node ./@graphql/schema/schema2json.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "storybook": "start-storybook -s ./storybook/public -p 8080 -c storybook",
        "build:storybook": "build-storybook -c storybook"
    },
